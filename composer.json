{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.1",
        "cviebrock/eloquent-sluggable": "^8.0",
        "doctrine/dbal": "^3.2",
        "dragon-code/api-response": "^9.0",
        "dragon-code/contracts": "^2.13",
        "dragon-code/laravel-migration-actions": "^2.1",
        "dragon-code/laravel-cache": "^2.9",
        "dragon-code/laravel-json-response": "^2.0",
        "dragon-code/laravel-support": "^3.1",
        "dragon-code/simple-data-transfer-object": "^2.0",
        "dragon-code/support": "^5.5",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.4",
        "http-interop/http-factory-guzzle": "^1.2",
        "laravel/framework": "^8.65",
        "laravel/sanctum": "^2.11",
        "laravel/tinker": "^2.5",
        "spatie/laravel-disable-floc": "^1.0"
    },
    "require-dev": {
        "dragon-code/env-sync-laravel": "^2.0",
        "dragon-code/pretty-routes": "^3.0",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9",
        "laravel-lang/http-statuses": "^2.0",
        "laravel-lang/publisher": "^11.1",
        "laravel/telescope": "^4.6",
        "mockery/mockery": "^1.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "app",
            "Database\\Factories\\": "database/factories",
            "Database\\Seeders\\": "database/seeders"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/*": true,
            "symfony/*": true
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --no-ansi --force",
            "php artisan lang:update --no-ansi",
            "php artisan env:sync --no-ansi",
            "php artisan telescope:publish --no-ansi",
            "php artisan optimize:clear --no-ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --no-ansi"
        ]
    }
}
