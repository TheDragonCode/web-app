{
    "name": "dragon-code/web-project",
    "description": "The Dragon Code Project.",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.1",
        "cviebrock/eloquent-sluggable": "^8.0",
        "dragon-code/laravel-support": "^3.1",
        "dragon-code/web-core": "^0.0.2"
    },
    "require-dev": {
        "dragon-code/env-sync-laravel": "^2.0",
        "dragon-code/pretty-routes": "^3.0",
        "facade/ignition": "^2.17",
        "fakerphp/faker": "^1.17",
        "laravel-lang/http-statuses": "^2.0",
        "laravel-lang/publisher": "^11.1",
        "laravel/telescope": "^4.6",
        "mockery/mockery": "^1.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5"
    },
    "repositories": [
        {
            "type": "github",
            "url": "git@github.com:TheDragonCode/web-core.git"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "app",
            "Database\\Factories\\": "database/factories",
            "Database\\Seeders\\": "database/seeders"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/*": true,
            "symfony/*": true
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "post-update-cmd": [
            "php artisan vendor:publish --tag=laravel-assets --force",
            "php artisan lang:update",
            "php artisan env:sync",
            "php artisan telescope:publish",
            "php artisan optimize:clear"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --no-ansi"
        ]
    }
}
